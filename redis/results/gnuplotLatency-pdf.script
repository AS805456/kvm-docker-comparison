set terminal pdf font "Times-Roman,9" linewidth 4 rounded fontscale 1.0
#set terminal pdfcairo font "Gill Sans,9" linewidth 4 rounded fontscale 1.0
set output "REDIS-LATENCY.pdf" 
set key top left
set datafile separator ","
set ylabel "Latency (ms)"
set xlabel "Number of client threads"
plot "Redis-latency.lf" using 1:($2/1000) with lines lt 1 title "Native", "Redis-latency.lf" using 1:($2/1000):($3/1000):($4/1000) with errorbars lt 1 notitle, "Redis-latency.lf" using 1:($7/1000) with lines lt 2 title "Docker Bridge", "Redis-latency.lf" using 1:($7/1000):($8/1000):($9/1000) with errorbars lt 2 notitle, "Redis-latency.lf" using 1:($12/1000) with lines lt 3 title "Docker Host", "Redis-latency.lf" using 1:($12/1000):($13/1000):($14/1000) with errorbars lt 3 notitle, "Redis-latency.lf" using 1:($17/1000) with lines lt 4 title "VM", "Redis-latency.lf" using 1:($17/1000):($18/1000):($19/1000) with errorbars lt 4 notitle, 
